{
  "env": {
    "browser": true, 
    "es6": true 
  },
  "extends": [
    "eslint:recommended", 
    "plugin:prettier/recommended" 
  ],
  "parserOptions": {
    "ecmaVersion": 2020, 
    "sourceType": "module" 
  },
  "rules": {
    "no-unused-vars": "warn", 
    "no-console": "error", 
    "eqeqeq": "error", 
    "no-var": "error", 
    "prefer-const": "error", 
    "no-debugger": "error", 
    "curly": "error", 
    "quotes": ["error", "single"], 
    "semi": ["error", "always"], 
    "indent": ["error", 2], 
    "no-multiple-empty-lines": ["error", { "max": 1 }], 
    "comma-dangle": ["error", "never"],
    "space-before-function-paren": ["error", "never"], 
    "no-trailing-spaces": "error", 
    "eol-last": ["error", "always"] 
  }
}
